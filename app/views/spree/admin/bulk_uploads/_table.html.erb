<table class="table" id='listing_prodict_exports'>
  <colgroup>
    <col style="width: 20%">
    <col style="width: 30%">
    <col style="width: 10%">
    <col style="width: 10%">
    <col style="width: 10%">
    <col style="width: 10%">
    <col style="width: 10%">
  </colgroup>
  <thead>
  <tr data-hook="admin_shipping_methods_index_headers">
    <th><%= Spree.t(:attachment_file_name) %></th>
    <th><%= Spree.t(:created_at) %></th>
    <th><%= Spree.t(:total_count) %></th>
    <th><%= Spree.t(:processed_count) %></th>
    <th><%= Spree.t(:progress) %></th>
    <th><%= Spree.t(:bulk_upload_errors) %></th>
    <th data-hook="admin_shipping_methods_index_header_actions" class="actions"></th>
  </tr>
  </thead>
  <tbody>
  <% (@bulk_uploads || []).each do |upload|%>
      <%= render partial: 'upload_row', locals: {upload: upload} %>
  <% end %>
  </tbody>
</table>